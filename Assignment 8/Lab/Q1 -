#include<iostream>
#include<queue>
using namespace std;

class node {
    public:
        int data;
        node* left;
        node* right;

    node(int d) {
        this -> data = d;
        this -> left = NULL;
        this -> right = NULL;
    }
};
node *buildtree(node *root){
    cout<<"enter the value ";
    int data;
    cin >>data;
    root = new node(data);

    if (data == -1){
        return NULL;
    }

    cout<<"Enter data to insert to the left of "<<data<<" ";
    root->left = buildtree(root ->left);
    cout<<"enter data to insert to right of "<<data<<" ";
    root->right =buildtree(root->right); 
    return root;
}

void inorder(node *root){
    if(root==NULL){
        return ;
    }

    inorder(root ->left);
    cout<<root -> data<<" ";
    inorder(root ->right);
}

void preorder(node *root){
    if(root==NULL){
        return ;
    }

    cout<<root -> data<<" ";
    preorder(root ->left);
    preorder(root ->right);

}
void postorder(node* root){
    if(root==NULL){
        return ;
    }

    preorder(root ->left);
    preorder(root ->right);
    cout<<root -> data<<" ";
}


int main(){
    node * root1;
    root1 = buildtree(root1);
    inorder(root1);
    preorder(root1);
    postorder(root1);
}


